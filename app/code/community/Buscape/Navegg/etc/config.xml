<config>
    <modules>
        <Buscape_Navegg>
            <version>0.1.0</version>
        </Buscape_Navegg>
    </modules>
    <global>
        <models>
            <navegg>
                <class>Buscape_Navegg_Model</class>
            </navegg>
        </models>
        <blocks>
            <navegg>
                <class>Buscape_Navegg_Block</class>
            </navegg>
        </blocks>
        <helpers>
            <navegg>
                <class>Buscape_Navegg_Helper</class>
            </navegg>
        </helpers>
    </global>
    <frontend>
        <events>
            <controller_action_layout_generate_blocks_after>
                <observers>
                    <navegg_page_head>
                        <class>navegg/observer</class>
                        <method>setPageHeadNavegg</method>
                    </navegg_page_head>
                </observers>
            </controller_action_layout_generate_blocks_after>
        </events>
    </frontend>


    <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <navegg_options>
                                            <title>Navegg</title>
                                        </navegg_options>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>


</config>
